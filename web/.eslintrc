{
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "env": {
      "browser": true,
      "node": true,
      "jest": true,
      "mocha": true
    },
    "parser": "babel-eslint",
    "rules": {
      "quotes": [1, "single"],
      "strict": [2, "never"],
      "semi": [0, "always"],
      "no-unused-vars": [1]
    },
    "extends": ["eslint:recommended", "airbnb"]
}
